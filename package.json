    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "^0.71.1",
    "prettier": "^2.7.1",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,tsx,ts}": [
      "npx eslint ./src/**/*.tsx --fix",
      "npx prettier --write ./src"
